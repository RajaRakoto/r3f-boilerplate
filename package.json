{
  "name": "r3f-boilerplate",
  "version": "1.0.0",
  "author": {
    "name": "raja",
    "email": "raja.rakoto7@gmail.com"
  },
  "description": "R3F boilerplate & starter project with Vite.js & Typescript",
  "homepage": "https://github.com/RajaRakoto/r3f-boilerplate",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --testPathIgnorePatterns src/tests/ignore",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore .",
    "format-check": "prettier --check --ignore-path .prettierignore .",
    "format-fix": "prettier --write --ignore-path .prettierignore .",
    "sseo-watch": "grunt --gruntfile sass-eo-cli.js watch-sass",
    "sseo-docs": "grunt --gruntfile sass-eo-cli.js sseo-docs",
    "sseo-magic": "grunt --gruntfile sass-eo-cli.js magic-docs",
    "sseo-buttons": "grunt --gruntfile sass-eo-cli.js buttons-maker"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "dependencies": {
    "@react-three/drei": "^9.62.4",
    "@react-three/fiber": "^8.12.0",
    "@react-three/postprocessing": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.151.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@raja_rakoto/sass-eo": "^1.8.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/three": "^0.150.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "axios-mock-adapter": "^1.21.4",
    "babel-loader": "^9.1.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "grunt": "^1.6.1",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-shell": "^4.0.0",
    "jest": "^29.5.0",
    "load-grunt-tasks": "^5.1.0",
    "prettier": "2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
